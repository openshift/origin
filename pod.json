{
  "kind": "Pod",
  "apiVersion": "v1beta1",
  "id": "openshift",
  "desiredState": {
    "manifest": {
      "version": "v1beta2",
      "containers": [
        {
          "name": "origin",
          "image": "openshift/origin",
          "ports": [{
              "containerPort": 8443,
          }],
          "command": ["start", "master"],
          "imagePullPolicy": "PullIfNotPresent"
        }
      ],
      "restartPolicy": {
        "always": {}
      }
    }
  }
}