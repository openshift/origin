                        "containers": [
                            {
                                "image": "openshift/docker-registry",
                                "name": "registry-container",
                                "ports": [
                                    {
                                        "containerPort": 5000,
                                        "protocol": "TCP"
                                    }
                                ]
                            }
                        ],
                        "id": "registryPod",
                        "version": "v1beta1",
                        "volumes": null
                    },
                    "restartpolicy": {}
