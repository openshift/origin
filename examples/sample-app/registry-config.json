{
    "id": "docker-registry-config",
    "kind": "Config",
    "apiVersion": "v1beta1",
    "creationTimestamp": "2014-09-18T18:28:38-04:00",
    "name": "docker-registry-config",
    "description": "Creates a private docker registry",
    "items": [
        {
            "apiVersion": "v1beta1",
            "creationTimestamp": null,
            "id": "registryservice",
