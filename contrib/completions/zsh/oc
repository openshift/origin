_oc_create_route()
{
    last_command="oc_create_route"
    commands=()
    commands+=("edge")
    commands+=("passthrough")
    commands+=("reencrypt")

    flags=()
    two_word_flags=()
    local_nonpersistent_flags=()
    flags_with_completion=()
