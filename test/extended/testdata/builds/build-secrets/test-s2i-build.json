{
  "kind": "BuildConfig",
  "apiVersion": "build.openshift.io/v1",
  "metadata": {
    "name": "test",
    "labels": {
      "name": "test"
    }
  },
  "spec": {
    "triggers": [],
    "source": {
      "type": "Binary",
      "binary": {},
      "secrets": [
        {
          "secret": {
            "name": "testsecret"
          },
          "destinationDir": "/tmp"
        },
        {
          "secret": {
            "name": "testsecret2"
          }
        }
      ],
      "configMaps": [
        {
          "configMap": {
            "name": "test-configmap"
          },
          "destinationDir": "/tmp/configmap"
        },
        {
          "configMap": {
            "name": "test-configmap-2"
          },
          "destinationDir": "configmap2"
        }
      ]
    },
    "strategy": {
      "type": "Source",
      "sourceStrategy": {
        "from": {
          "kind": "DockerImage",
          "name": "image-registry.openshift-image-registry.svc:5000/openshift/ruby:3.1-ubi8"
        },
        "env": [
          {
            "name": "BUILD_LOGLEVEL",
            "value": "5"
          }
        ]
      }
    },
    "output": {
      "to": {
        "kind": "ImageStreamTag",
        "name": "test:latest"
      }
    }
  }
}
